{
  "name": "bsbll",
  "version": "0.0.1",
  "description": "Fan made baseball web app.",
  "main": "app.js",
  "scripts": {
    "start:server": "node build/app.js | bunyan -o short -l info --time local",
    "start": "npm run build && npm run start:server",
    "build": "tsc",
    "clean:build": "rm -rf build/*",
    "clean:dep": "rm -rf node_modules",
    "clean:all": "npm run clean:dep && npm run clean:build"
  },
  "keywords": [
    "mlb",
    "baseball"
  ],
  "author": "Evan Fossier",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^7.0.5",
    "@types/sprintf-js": "0.0.27",
    "bunyan": "^1.8.5",
    "bunyan-morgan": "^1.0.1",
    "express": "^4.14.1",
    "node-cron": "^1.1.3",
    "process": "^0.11.9",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "sprintf-js": "^1.0.3",
    "typescript": "^2.3.0-dev.20170226",
    "web-request": "^1.0.7"
  },
  "devDependencies": {
    "@types/react": "^15.0.12"
  }
}
